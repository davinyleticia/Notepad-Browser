<VirtualHost *:80>
    ServerAdmin admin@example.com
    ServerName file.notepad
    ServerAlias file.notepad
    DocumentRoot /opt/Notepad-Browser/file/
    ErrorLog ${APACHE_LOG_DIR}/error.log
    CustomLog ${APACHE_LOG_DIR}/access.log combined

</VirtualHost>

